{
  "name": "K3YRING [beta]",
  "version": "0.6.28",
  "manifest_version": 2,
  "minimum_chrome_version": "20",
  "content_security_policy": "default-src 'self'; connect-src 'self'; style-src 'self' 'unsafe-inline'",
  "description":"Your Passwords: Local and Portable",
  "icons": {
         "19": "icons/icon19.png",
         "48": "icons/icon48.png",
	     "128": "icons/fullIcon48.png",
	     "340": "icons/fullIcon128.png"
	   },
  "permissions": ["contextMenus", "tabs", "webRequest", "webNavigation", "notifications"],
  "background": {"page": "bp_Main.html"},
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*", "ftp://*/*", "file://*/*"],
    "exclude_matches": ["*://chrome.google.com/webstore*"],
    "js": ["bp_cs_boot.cat.js"],
    "run_at": "document_idle",
    "all_frames": true
    }
  ],
  "#content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*", "ftp://*/*", "file://*/*"],
    "exclude_matches": ["*://chrome.google.com/webstore*"],
    "js": ["tp/jquery.js",
           "tp/jquery-ui.js",
           "bp_build.js",
           "bp_cs_boot_head.js",
           "bp_cs.cat.js",
           "bp_cs_boot_stub.js"],
    "run_at": "document_idle",
    "css": ["bp.css"],
    "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "bp_panel.html"
  },
  "options_page": "bp_manage.html",
  "offline_enabled": true,
  "web_accessible_resources": [
    "icons/icon128.png",
    "icons/icon48.png",
    "icons/icon16.png",
    "tp/bootstrap/img/glyphicons-halflings.png",
    "tp/bootstrap/img/glyphicons-halflings-white.png",
    "tp/jquery.js",
    "tp/jquery-ui.js",
    "bp_cs.cat.js",
    "bp.css",
    "bp_cs_boot.cat.js"],
  "incognito": "spanning"
}
